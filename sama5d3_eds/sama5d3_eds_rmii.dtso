/dts-v1/;
/plugin/;

#include "dt-bindings/interrupt-controller/irq.h"
#include "dt-bindings/pinctrl/at91.h"

&macb1 {
	phy-mode = "rmii";
	phy-handle = <&mbitphy>;
	max-speed = <100>;
	status = "okay";

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		mbitphy: ethernet-phy@0 {
			/* LAN8740 */
			compatible = "ethernet-phy-id0007.c110";
			reg = <0x0>; 
			max-speed = <100>;
			status = "okay";
		};
	};
};

&{/} {
	model = "SAMA5D3-EDS: Generic RMII PHY";
};
